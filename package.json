{
  "name": "pup-ai-v2",
  "version": "2.0.0",
  "description": "Context-aware Slack bot with personality and memory",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "npm run db:migrate && node dist/bootstrap.js",
    "start:prod": "node dist/bootstrap.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "quality": "npm run typecheck && npm run lint && npm run test",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e --runInBand",
    "test:performance": "tsx scripts/performance-tests.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:mutation": "stryker run",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "test:security": "tsx scripts/security-test.ts",
    "db:create": "node-pg-migrate create --migration-file-language sql",
    "db:migrate": "node-pg-migrate up",
    "migrate:prod": "node scripts/migrate.js",
    "db:rollback": "node-pg-migrate down",
    "db:reset": "node-pg-migrate reset",
    "db:seed": "tsx scripts/seed.ts",
    "db:optimize": "tsx scripts/runDatabaseOptimization.ts",
    "db:benchmark": "tsx src/db/benchmarks/performanceBenchmark.ts",
    "db:monitor": "tsx src/db/monitoring/databaseMonitor.ts",
    "migrate:ai-optimize": "tsx scripts/migrate-to-optimized-ai.ts",
    "ai:report": "curl http://localhost:3000/ai/report",
    "ai:health": "curl http://localhost:3000/ai/health",
    "debug": "node --inspect -r tsx/cjs src/index.ts",
    "profile": "node --prof -r tsx/cjs src/index.ts",
    "analyze": "npm run build && npx webpack-bundle-analyzer dist/stats.json",
    "prepare": "husky install",
    "commit": "cz",
    "test:performance": "tsx src/tests/performance/benchmark.ts",
    "benchmark": "tsx src/tests/performance/benchmark.ts",
    "load-test": "tsx src/tests/performance/loadTest.ts",
    "workers": "tsx src/workers/manager.ts",
    "workers:prod": "node dist/workers/manager.js",
    "cache:warm": "tsx scripts/warmCache.ts",
    "costs:report": "tsx scripts/costReport.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/businesswalrus/pupper.git"
  },
  "keywords": [
    "slack",
    "bot",
    "ai",
    "typescript",
    "bolt"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/businesswalrus/pupper/issues"
  },
  "homepage": "https://github.com/businesswalrus/pupper#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@slack/bolt": "^4.4.0",
    "@types/pg": "^8.15.4",
    "axios": "^1.9.0",
    "bullmq": "^5.53.2",
    "cheerio": "^1.1.0",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.3.2",
    "lru-cache": "^10.0.1",
    "lz4js": "^0.2.0",
    "openai": "^4.104.0",
    "p-limit": "^3.1.0",
    "pg": "^8.16.0",
    "pgvector": "^0.2.1",
    "redis": "^4.7.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.1",
    "@faker-js/faker": "^9.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "commitizen": "^4.3.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.2.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.3.1",
    "node-pg-migrate": "^6.2.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  }
}
