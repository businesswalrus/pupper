# Slack Configuration
SLACK_BOT_TOKEN=xoxb-your-bot-token
SLACK_APP_TOKEN=xapp-your-app-token  
SLACK_SIGNING_SECRET=your-signing-secret

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/pupper
REDIS_URL=redis://localhost:6379

# AI Services
OPENAI_API_KEY=sk-your-openai-key

# Application Settings
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# Feature Flags (comma-separated)
FEATURE_FLAGS=search,profiling,mcp_extended,debug_mode

# Rate Limiting
OPENAI_RATE_LIMIT=60
SLACK_RATE_LIMIT=20

# Optional: Test Database
DATABASE_TEST_URL=postgresql://postgres:postgres@localhost:5432/pupper_test

# Optional: Sentry Error Tracking
SENTRY_DSN=

# Optional: Analytics
ANALYTICS_ENABLED=false